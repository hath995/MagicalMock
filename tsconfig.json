{
    "compilerOptions": {
        "module": "commonjs",
        "sourceMap": true,
        "target": "ES6",
        "noImplicitAny": true,
        "allowJs": true,
        "allowUnreachableCode": false,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturn": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules"
    ]
}
